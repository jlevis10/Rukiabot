api = tweepy.API(auth)


#defining function
def tweet(tweet_text):
    api.update_status(tweet_text)
    print('Tweeted: ' + tweet_text)
    time.sleep(60)




tweet('Hello World!')
         

#defining loop to tweet
while True:
    tweet('Hello World!')
    time.sleep(random.randint(60, 300))


#class which generates words to be tweeted using the tweet function
 
 
